[flake8]
# Configuration moved to pyproject.toml
# This file kept for tools that don't support pyproject.toml yet
max-line-length = 150
extend-ignore = E203, E266, E501, E731, W292, W391, E402, W503, W504, DJ01
exclude = 
    .git,
    __pycache__,
    */__init__.py,
    experimental_classifiers/*,
    migrations,
    .venv,
    build,
    dist
